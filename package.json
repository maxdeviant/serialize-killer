{
  "name": "serialize-killer",
  "version": "1.0.0",
  "repository": "git@github.com:maxdeviant/serialize-killer.git",
  "author": "Marshall Bowers <elliott.codes@gmail.com>",
  "license": "MIT",
  "description": "Make your objects unserializable.",
  "keywords": [
    "serialize",
    "serialization",
    "unserializable",
    "poison",
    "serial",
    "killer"
