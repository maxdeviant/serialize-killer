  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write '**/*.+(js|json|md|ts|tsx)'",
    "build": "tsc -p .",
    "test": "ava"
  },
  "files": [
    "lib/",
    "!lib/**/__tests__/",
    "!lib/**/*.test.{js,d.ts}"
  ],
  "devDependencies": {
    "@types/node": "18.15.9",
    "ava": "5.2.0",
    "prettier": "2.8.7",
    "safe-stable-stringify": "2.4.3",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  }
}
